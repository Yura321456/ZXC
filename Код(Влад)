#include <stdio.h>
#include <stdlib.h>

void main(){
int size;
printf("введіть розмір ");
    scanf("%i",&size);
int A[size];
int pair = 0;
int deltSize = 0;
double C[size - deltSize];

printf("Введіть значення масиву ");

while(pair < 2)
{
    deltSize = 0; 
    for( int i = 0; i < size; i++){
      scanf("%i",&A[i]);
    printf("A(%i) = %i\n",i, A[i]);
}
    
    for( int j = 0;j < size ;j++){
      if(A[j] % 2==1){
        pair ++;
        }
        if(pair == 2){
            C[0] = A[j];
     printf(" C(0) = %lf\n",C[0]);
     break;
        }
deltSize++;

}
     
}
    

for (int i = 1; i < size - deltSize; i++) {
   C[i] = A[deltSize + i] / C[0];
   printf("C[%i] = %lf\n",i,C[i]);
}
}

