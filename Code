#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void main(){
    srand(time(NULL));
int A[12];
int pair = 0;
int deltSize = 0;
double C[12 - deltSize];

for( int i = 0; i < 12; i++){
    A[i] = rand() % 100;
    printf("A(%i) = %i\n",i, A[i]);
}
for( int j = 0;j < 12 ;j++){
      if(A[j] % 2==1){
        pair++;
        }
        if(pair == 2){
            C[0] = A[j];
     printf(" C(0) = %lf\n",C[0]);
     break;
     
}
deltSize++;
}
for (int i = 1; i < 12 - deltSize; i++) {
   C[i] = A[deltSize + i];
   printf("C[%i] = %lf\n",i,C[i]);
}
}
