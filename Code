#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void main(){
    srand(time(NULL));
int A[12];
int pair = 0;
int outC = 0;
int C[outC];

for( int i = 0; i < 12; i++){
    A[i] = rand() % 100;
    printf("A(%i) = %i\n",i, A[i]);
}
for( int j = 0;j < 12 ;j++){
      if(A[j] % 2==1){
        pair++;
        }
        if(pair >= 2){
            C[outC] = A[j];
     printf(" C(%i) = %i\n",j,C[outC]);
}
}
}
