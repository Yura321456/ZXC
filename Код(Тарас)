#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void main(){
    srand(time(NULL));
int A[12];
int pair = 0;
int deltSize = 0;
double C[12 - deltSize];



while(pair < 2)
{
    deltSize = 0; 
 
    for( int i = 0; i < 12; i++){
    A[i] = 2 + rand() % 100;
 
}
    
    for( int j = 0;j < 12 ;j++){
      if(A[j] % 2==1){
        pair ++;
        }
        if(pair == 2){
            C[0] = A[j];
    
     break;
        }
deltSize++;

}
     
}
    printf("A = { ");
for (int i = 0; i < 12; i++) {
    printf("%i ",A[i]);
}
printf("}\n");
printf("C = { ");
printf("%.3lf ",C[0]);
for (int i = 1; i < 12 - deltSize; i++) {
   C[i] = A[deltSize + i] / C[0];
   printf("%.3lf ",C[i]);
}
printf("}");
}
