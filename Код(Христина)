#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void main(){
    srand(time(NULL));
int n = 1;
int A[n];
int pair = 0;
int deltSize = 0;
double C[12 - deltSize];

printf("Введіть кількість елементів масиву ");
scanf("%i",&n);
while(pair < 2)
{
    deltSize = 0; 
    for( int i = 0; i < n; i++){
    A[i] = 2 + rand() % 100;
    printf("A(%i) = %i\n",i, A[i]);
}
    
    for( int j = 0;j < n;j++){
      if(A[j] % 2==1){
        pair ++;
        }
        if(pair == 2){
            C[0] = A[j];
     printf(" C(0) = %lf\n",C[0]);
     break;
        }
deltSize++;

}
     
}
    

for(int i = 1; i < n - deltSize; i++) {
   C[i] = A[deltSize + i] / C[0];
   printf("C[%i] = %lf\n",i,C[i]);
}
}
